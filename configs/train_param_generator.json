{
    "exp_name": "param_generator",
    "data_dir": "/path/to/dataset/graphs",
    "train_dataset": "/path/to/dataset/graphs/train_graphs_v2.txt",
    "val_dataset": "/path/to/dataset/graphs/val_graphs_v2.txt",
    "node_type_list": "/path/to/dataset/graphs/node_types_quantized_macro.json",
    "model_dir": "/path/to/result/pretrain/models",
    "log_dir": "/path/to/result/pretrain/logs",
    "node_order": "reverse_breadth_first_flipped_no_auxiliary_nodes",
    "use_alpha": true,
    "image_ext": "jpg",
    "max_num_nodes": 80,
    "max_num_parents": 26,
    "max_num_node_params": 100,
    "node_param_quant_steps": 128,
    "max_node_param_vec_dim": 8,
    "max_node_param_seq_len": 75,
    "max_full_seq_len": 1250,
    "hidden_dim": [48, 96],
    "num_layers": [4, 4],
    "num_heads": [4, 4],
    "num_gcn": 6,
    "edge_condition": "node_edge_gnn",
    "num_workers": 4,
    "data_chunksize": 128,
    "target_shuffle_queue_size": 1024,
    "epochs": 10,
    "lr": 1e-4,
    "weight_decay": 1e-4,
    "grad_clip_norm": 1.0,
    "lr_schedule_type": "cosine_warmup",
    "lr_schedule_warmup": 1200,
    "lr_schedule_annealing": 780000,
    "batch_size": 56,
    "devices": ["cuda:0"],
    "image_encoder_type": "clip",
    "clip_model": "ViT-L/14",
    "normalize_clip": true,
    "pil_image_loader": true,
    "validation_interval": 36000,
    "checkpoint_interval": 2,
    "ignore_padding": true,
    "distributed": true,
    "embed_type": null,
    "use_encoder_decoder": false,
    "cond_type": "cross_attention",
    "allow_tf32": true
}
